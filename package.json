{
  "name": "ts-express-kit",
  "version": "1.1.4",
  "description": "A TypeScript starter kit for building Express applications with best practices.",
  "main": "index.js",
  "bin": {
    "ts-express-kit": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ignore-path .eslintignore --ext .ts",
    "lint:fix": "npx eslint src --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "npx prettier --write src",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "prod": "node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "express",
    "node",
    "ts-express-kit"
  ],
  "author": "Nasim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nazmulhasannasim333/ts-express-kit.git"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-yup-middleware": "^2.0.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "prisma": "^6.14.0",
    "uuidv4": "^6.2.13",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
